[Unit]
Description=Autogenerated unit for Procfile type {{key}}

[Service]
ExecStart=/var/juiz/remote/run-app "{{value|e}}"
WorkingDirectory=/app
User=app
ReadOnlyDirectories=/
ReadWriteDirectories=/tmp
Environment="PORT=21645"
{%- if key == "web" %}
Restart=on-failure
{%- endif %}

{%- if key == "web" %}
[Install]
WantedBy=multi-user.target
{%- endif %}
